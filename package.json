{
    "name": "webpack-logrocket-plugin",
    "version": "0.0.0",
    "description": "SDK Integrations",
    "keywords": [
        "moengage",
        "integrations"
    ],
    "author": "rishabhgupta <rishabh@moengage.com>",
    "homepage": "https://github.com/moengage/dashboard-commons#readme",
    "license": "MIT",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "lib"
    ],
    "publishConfig": {
        "access": "private"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moengage/dashboard-commons.git"
    },
    "scripts": {
        "prebuild": "rimraf ./build",
        "build": "npx typedoc",
        "lint": "eslint './src/**/*.{ts,tsx}'",
        "upload": "node ../../scripts/s3upload.js core",
        "prelib": "rimraf ./lib",
        "lib": "tsc --declaration",
        "ts-check": "tsc --noEmit",
        "format:check": "prettier --check \"./src/**/*.{ts,tsx,scss}\"",
        "format": "prettier --write \"./src/**/*.{ts,tsx,scss}\""
    },
    "bugs": {
        "url": "https://github.com/rishabhgupta/moengage-js/issues"
    },
    "dependencies": {
        "logrocket-cli": "^0.12.0"
    },
    "peerDependencies": {
        "webpack": "^4.0.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^5.16.0",
        "prettier": "2.0.5",
        "rimraf": "^2.6.3",
        "typedoc": "^0.17.1",
        "typescript": "^3.8.3"
    }
}
